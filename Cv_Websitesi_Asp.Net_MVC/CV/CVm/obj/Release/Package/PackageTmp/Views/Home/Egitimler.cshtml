@using CV.Models
@model List<Egitim>
<div class="shadow-1-strong bg-white my-5 p-5" id="education">
    <div class="education-section">
        <h2 class="h2 fw-light mb-4">Eğitim</h2>
        <div class="timeline">
            @foreach (var x in Model)
            {
                <div class="timeline-card timeline-card-success" data-aos="fade-in" data-aos-delay="0">
                    <div class="timeline-head px-4 pt-3">
                        <div class="h5">@x.Okul <span class="text-muted h5">/ @x.Bolum</span>          </div>
                    </div>
                    <div class="timeline-body px-4 pb-4">
                        <div class="text-muted text-small mb-3">@x.BaslangicTarih - @x.BitisTarih</div>
                        <div>@x.Aciklama</div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
